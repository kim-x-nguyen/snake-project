import java.awt.Graphics;

import javax.swing.JPanel;

public class GameScreen extends JPanel {
	int a = 0;


	
	public GameScreen() {


	}
	public void paint(Graphics g) {
		g.fillRect(10, 10, 100, 100);
	}
	
	public void run() {
		while(true) {
			a++;
			System.out.println(String.valueOf(a));
		}
	}

}
